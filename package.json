    "prettier": "^3.4.2",
    "supertest": "^7.0.0",
    "vite": "^6.0.1"
  },
  "dependencies": {
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-handlebars": "^8.0.1",
    "marked": "^15.0.6",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.1.9",
    "sass": "^1.83.0"
